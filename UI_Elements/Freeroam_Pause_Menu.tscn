[sub_resource type="GDScript" id="GDScript_rnwtl"]
script/source = "extends MarginContainer


func _on_close_pressed():
	get_tree().call_group(\"when_paused\", \"_on_game_paused\")

func update_intel(chara:Character):
	%Portrait.texture = chara.portrait
	%Stats.text = \"\\n\".join( [chara.human_name().to_upper(),
					\"Health: \" + str(chara.max_health),
					\"Speed: \" + str(chara.speed),
					\"Will: \" + str(chara.max_mental),
					\" \"] )
	%Trivia.text = \"\\n\".join( [
					\"RANK: \" + chara.lore.get_rank_name(),
					\"ALIAS: \" + chara.lore.org_codename,
					\"BLOOD: \" + chara.lore.bloodtype,
					\"GENDER: \" + chara.lore.sex.to_lower(),
					\" \" ] )
	%Access.text = \"\\n\".join(chara.access_clearance)
"

