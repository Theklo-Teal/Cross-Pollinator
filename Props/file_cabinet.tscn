script/source = "extends InteractProp

var open : bool
var anim :Tween

func make_use():
	open = not open
	anim = create_tween()
	anim.set_loops(1)
	if open:
		anim.tween_property($Geometry/Drawer, \"position:z\", -0.25, 0.3).from(-0.05)
		anim.play()
	else:
		anim.tween_property($Geometry/Drawer, \"position:z\", -0.05, 0.3).from(-0.25)
		anim.play()
"

